SELECT * FROM allthings_apparel;
SELECT * FROM allthings_apparel ORDER BY aisle; 
SELECT * FROM allthings_apparel ORDER BY quantity; 
SELECT * FROM allthings_apparel ORDER BY price;
SELECT SUM(price) FROM allthings_apparel; 
